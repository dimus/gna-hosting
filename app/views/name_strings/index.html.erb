<h1>Listing name_strings</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Is canonical</th>
    <th>Canonical</th>
  </tr>

<% for name_string in @name_strings %>
  <tr>
    <td><%=h name_string.name %></td>
    <td><%=h name_string.is_canonical ? "yes" : "no"  %></td>
    <td><%=h name_string.canonical_id %></td>
    <td><%= link_to 'Show', name_string %></td>
    <td><%= link_to 'Edit', edit_name_string_path(name_string) %></td>
    <td><%= link_to 'Destroy', name_string, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New name_string', new_name_string_path %>
