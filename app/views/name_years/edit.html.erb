<h1>Editing name_year</h1>

<% form_for(@name_year) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name_year %><br />
    <%= f.text_field :name_year %>
  </p>
  <p>
    <%= f.label :is_confirmed %><br />
    <%= f.check_box :is_confirmed %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @name_year %> |
<%= link_to 'Back', name_years_path %>
