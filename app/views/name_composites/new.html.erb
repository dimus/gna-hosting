<h1>New name_composite</h1>

<% form_for(@name_composite) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name_string %><br />
    <%= f.text_field :name_string %>
  </p>
  <p>
    <%= f.label :name_author %><br />
    <%= f.text_field :name_author %>
  </p>
  <p>
    <%= f.label :name_year %><br />
    <%= f.text_field :name_year %>
  </p>
  <p>
    <%= f.label :is_confirmed %><br />
    <%= f.check_box :is_confirmed %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', name_composites_path %>
